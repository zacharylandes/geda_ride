  <h1>Events near your destination </h1>
  <table class= "table " id= 'ride-list' >
      <tr>
        <th >Event</th>
        <th >City</th>
        <th >Date</th>
        <th >Visit</th>

      </tr>
        <% @events.each do |event| %>
        <tr >
          <td><%=  event.title %>
          <td><%= link_to params['destination'] %>
          <td><%= event.date %>
          <td><%=link_to image_tag(events), event.url, class: 'event-image' %></td>
        </tr>
        <% end %>
  </table>
